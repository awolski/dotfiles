{
    "vim.useCtrlKeys": true,
    "vim.vimrc.enable": true,
    "vim.visualModeKeyBindings": [
    
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "editor.tabSize": 2,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.scrollback": 10000,
    "diffEditor.renderSideBySide": false
}
