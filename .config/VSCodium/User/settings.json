{
    "vim.useCtrlKeys": true,
    "vim.vimrc.enable": true,
    "vim.visualModeKeyBindings": [
    
    ],
    "window.zoomLevel": 0,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "editor.tabSize": 2,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    }
}
